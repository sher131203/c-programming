#include<stdio.h>
int main()
{
  char gender='M';
  int age=23;
  if (gender=='M' || gender=='m')
  {
      if(age>=18)
      {
          printf("Eligible Bachelore....");
      }
      else
      {
          printf("Not Eligible....");
      }
  }
      else if(gender=='F' || gender=='f')
      {
          if(age>=18)
          {
              printf("Eligible Spinster...");
          }
          else
          {
              printf("Not Eligible...");
          }
      }
      else
      {
          printf("Please check the given details");
      }
          
  }  
