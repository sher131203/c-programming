#include <stdio.h>
 
int main()
{
  int n;
  int a = 0; 
  int b = 1;
  int result; 
  int i;
  printf("ENTER THE VALUE:");
  scanf("%d", &n);
  printf("Fibonacci Series is: \n");
  for (i = 0; i < n; i++)
  {
    if (i <1)
    {
       
        result = i;
     
    }
     
    else
    {
        result = a + b;
       a= b;
      b = result;
    }
    printf("%d \n", result);
  } 
  return 0;
} 
