#include <stdio.h>

int main() 
{
    int a[3][3]={{2,4,8},{9,11,7},{6,4,5}};
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
        if(a[i][j]%2==0)
        {
            printf("The even no is:%d\n",a[i][j]);
        }
    
        else
        {
          printf("The odd no is:%d\n",a[i][j]);
        }
        }
    }
    return 0;
}
